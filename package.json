{
  "name": "qqlite",
  "version": "1.0.0",
  "description": "a light version of qq",
  "main": "index.ts",
  "author": "Dreagonmon",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "graphql": "^16.3.0",
    "graphql-ws": "^5.6.1",
    "oicq": "^2.2.0",
    "uWebSockets.js": "uNetworking/uWebSockets.js#20.6.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.19",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.9.0",
    "typescript": "^4.5.5"
  },
  "scripts": {
    "start": "node dist/index.js",
    "watch": "tsc -w",
    "build": "npm run lint && tsc --build",
    "clean": "tsc --build --clean",
    "lint": "eslint index.ts schema scripts --fix",
    "build-schema": "tsc --build && node dist/scripts/get-graphql-schema.js > static/schema.graphql"
  }
}
